}
.N_LoginForm dl dd input{
	border:none !important;
	font-family:"微软雅黑";
	font-size:15px;
	color:#ffffff;
	background:none !important;
	height:35px !important;
	margin:0px !important;
	padding:0px !important;
	box-shadow:none !important;
	width:100%;
}
.N_LoginForm dl dd input:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color:#FEEBDA; opacity:1; 
}

.N_LoginForm dl dd input::-moz-placeholder { /* Mozilla Firefox 19+ */
    color:#FEEBDA;opacity:1;
}

.N_LoginForm dl dd input:-ms-input-placeholder{
    color:#FEEBDA;opacity:1;
}

.N_LoginForm dl dd input::-webkit-input-placeholder{
    color:#FEEBDA;opacity:1;
}
:-webkit-autofill {
 -webkit-text-fill-color: #FEEBDA !important;
 transition: background-color 5000s ease-in-out 0s !important;
}
