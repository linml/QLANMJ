<{include file = "simple_header.tpl"}>

<div class="N_Header" style="background-image:url(/assets/images/agentCenter/headerBg4.png);">
    <a href="javascript:history.go(-1);" class="ReturnUp">返回首页</a>
    <div class="N_Header_title">账户安全</div>
</div>
<div class="N_Wrap" style="bottom:0;">
    <div class="N_WrapAuto wp_94 mgTp3A">
        <div class="N_Pad">
            <form action="javascript:;" id="passwordfrm">
            <div class="N_AddForm N_AddForm2">
                <dl>
                    <dt>当前密码</dt>
                    <dd><input type="password" id="prepsw" placeholder="请输入当前密码"></dd>
                </dl>
                <dl>
                    <dt>设置密码</dt>
                    <dd><input type="password" id="newpsw" placeholder="请输入您的新密码"></dd>
                </dl>
                <dl>
                    <dt>确认密码</dt>
                    <dd><input type="password" id="confirmpsw" placeholder="请确认您的新密码"></dd>
                </dl>
            </div>
            <div class="NbcBtn"><button type="button" id="updatePsw">确认修改</button> </div>
            </form>
        </div>
    </div>
</div>