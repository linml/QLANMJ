.mui-backdrop {
    position: fixed;
    z-index: 998;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background-color: rgba(0,0,0,.3);
}

.mui-dtpicker-header button {
	background: #f5f5f5;
	border: solid 1px #ccc;
}

.mui-dtpicker-header button:last-child {
	background: #039bfe;
	border: solid 1px #0199FE;
	color: #fff;
}