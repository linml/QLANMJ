body,div,span,dl,dd,h1,h2,h3,h4,h5,h6,p,form,pre,input,button,textarea,select,ol,li,ul,td,th,img,em,i,b,table,td,th,section,article,aside,header,footer,nav,dialog,figure,a
{
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    -ms-box-sizing: border-box;
    -o-box-sizing: border-box;
    box-sizing: border-box;
}

html,body { font-size: 100%; line-height: 1.5 }

a,.shelter_filter_pop {
-webkit-tap-highlight-color: rgba(0,0,0,0);}
em,i,b{font-weight:normal;font-style:normal}
input,button,select,textarea{ outline:none;}
textarea{  resize:none;}

.bb1 { border-bottom: solid 1px #f0f0f0; }
.bb2 { border-bottom: solid 1px #d8d8d7; }

.ell {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.cor_4 { color: #039BFE; }
.cor_5 { color: #FF5D20; }

body {
	/*background: #F9F7FB;*/
  background-color: #ececf1;
  /*padding-top: 50px;*/
}

.bgf { background: #fff; }

.bg1 {
  background: #fff;
  border: solid 1px #F3F2F4;
  border-radius: 5px;
}

.bs1 {
	box-shadow: 0 0 3px 0 rgba(0, 0, 0, 0.05);
}

.ipt_out {
	border: solid 1px #ccc;
	border-radius: 5px;
	height: 34px;
}

.ipt_1 {
  width: 85px;
	background: #fff;
	border: none;
	line-height: 30px;
	border-radius: 5px;
	padding-left: 5px;
	font-size: 14px;
}


.ico_date {
	display: inline-block;
	width: 15px;
	height: 14px;
	background: url(../../images/agentCenter/ico_date.png) no-repeat;
	background-size: 100%;
	vertical-align: middle;
}

.btn_1 {
	display: inline-block;
	padding: 0 6px;
	line-height: 34px;
	border-radius: 5px;
	background: -webkit-linear-gradient(left, #25BFFF , #0199FE); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(right, #25BFFF , #0199FE); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(right, #25BFFF , #0199FE); /* Firefox 3.6 - 15 */
  background: linear-gradient(to right, #25BFFF , #0199FE); /* 标准的语法 */
  color: #fff;
}

.table_1 {
	width: 100%;
}

.table_1 th {
	font-weight: normal;
	color: #666;
	font-size: 14px;
	line-height: 45px;
	border-bottom: solid 1px #F0F0F0;
}

.table_1 td {
  padding: 10px;
	font-size: 12px;
	line-height: 24px;
	border-bottom: solid 1px #F0F0F0;
}

.table_1 tr:last-child  td {
	border: none;
}

.ico_load {
	display: inline-block;
	width: 14px;
	height: 14px;
	background: url(../images/ico_load.png) no-repeat;
	background-size: 100%;
	vertical-align: middle;
}

.table_2 {
	margin: -1px;
	width: 100%;
  text-align: center;
}

.table_2 td {
	border: solid 1px #f0f0f0;
	line-height: 82px;
	font-size: 16px;
	font-weight: 800;
	color: #666;
}

.table_2 .current {
	background: -webkit-linear-gradient(left, #25BFFF , #0199FE); 
  background: -o-linear-gradient(right, #25BFFF , #0199FE); 
  background: -moz-linear-gradient(right, #25BFFF , #0199FE); 
  background: linear-gradient(to right, #25BFFF , #0199FE); 
  color: #fff;
}

.list_1 li {
	line-height: 50px;
	padding: 0 15px;
  border-bottom: solid 1px #f0f0f0;
}

.list_1 li:last-child {
	border: none;
}

.btn_2 {
	display: inline-block;
	line-height: 46px;
	background: -webkit-linear-gradient(left, #0199FE , #25BFFF); 
  background: -o-linear-gradient(right, #0199FE , #25BFFF); 
  background: -moz-linear-gradient(right, #0199FE , #25BFFF); 
  background: linear-gradient(to right, #0199FE , #25BFFF); 
  color: #fff;
  border-radius: 5px;
  font-size: 16px;
  text-align: center;
}

.btn_3 {
	display: inline-block;
	line-height: 46px;
